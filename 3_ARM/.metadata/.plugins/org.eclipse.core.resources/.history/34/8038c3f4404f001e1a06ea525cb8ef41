#ifndef MRCC_INTERFACE_H_
#define MRCC_INTERFACE_H_

/*******************************************************************************************/
/* Reference Manual 2.3 Memory map (Page 38)                                               */
/* We have 4 Buses Interface which are :                                                   */
/*                                      - AHB1                                             */
/*                                      - AHB2                                             */
/*                                      - APB1                                             */
/*                                      - APB2                                             */
/*******************************************************************************************/
#define  RCC_APB1               0
#define  RCC_APB2               1
#define  RCC_AHB1               2
#define  RCC_AHB2               3




#endif /* MRCC_INTERFACE_H_ */
